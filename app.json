{
  "expo": {
    "name": "EduPlay Offline",
    "slug": "eduplay-offline",
    "scheme": "eduplay-offline",
    "version": "1.0.3",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eduplay.offline",
      "icon": "./assets/logo.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#4CAF50"
      },
      "package": "com.eduplay.offline",
      "permissions": [],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/logo.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "enableMinifyInReleaseBuilds": true
          }
        }
      ],
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "bdf32784-bfd4-42a7-8083-6762275f7d6b"
      }
    },
    "owner": "dswddevopscursor"
  }
}
